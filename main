print("why the fuck does github take forever to update sometimes :/")

-- ðŸ˜­
local StarterGui = game:GetService("StarterGui")
local Notify = function(_Title, _Text, Time)
    StarterGui:SetCore("SendNotification",{
        Title = _Title,
         Text = _Text,
         Icon = "rbxassetid://2541869220",
         Duration = Time
    })
end

-- mv = main version
-- cv = current version
writefile("version.txt",game:HttpGet("https://raw.githubusercontent.com/abhisheknaiiduh/loading/main/version.txt"))
local mv = readfile('version.txt')
local cv = "23"

if cv ~= mv then 
game.Players.LocalPlayer:Kick("OUTDATED VERSION,PLEASE UPDATE. Error code:3")
elseif cv == mv then
loadstring(game:HttpGet("https://raw.githubusercontent.com/abhisheknaiiduh/loading/main/key",true))()
end
Notify("alert!","Config is now free! no more whitelist",10)
