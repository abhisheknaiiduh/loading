local info = {Enum.EasingDirection.InOut,Enum.EasingStyle.Sine,.1,true}
Close.MouseButton1Click:Connect(function()
	if ChatGui.Position.X.Offset == 10 then
		info = table.clone(info)
		info[5] = function()
			Close.Image = "http://www.roblox.com/asset/?id=6031094680"
		end
		ChatGui:TweenPosition(UDim2.new(0, -(xbase+1),0.28, -10),unpack(info))
	elseif ChatGui.Position.X.Offset == -(xbase+1) then
		info = table.clone(info)
		info[5] = function()
			Close.Image = "http://www.roblox.com/asset/?id=6031094670"
		end
		ChatGui:TweenPosition(UDim2.new(0, 10,0.28, -10),unpack(info))
	end
end)

