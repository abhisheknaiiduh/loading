ts.MessageReceived:Connect(function(z)
    local speaker = tostring(z.TextSource)
    local loweredversion = speaker:lower()
    if z.Metadata ~= "" then print(z.Metadata)end
    if speaker == owner then
    creatmsg("<font color='rgb(152, 194, 219)'>[SEXER]</font> <font color='rgb(0, 255, 0)'>Rev</font>:",z.Text)
    elseif speaker == xerv then
    creatmsg("<font color='rgb(152, 194, 219)'>[AC MOD]</font> xerv:",z.Text)
    elseif speaker == kos then
    creatmsg("<font color='rgb(152, 0, 0)'>[adolf hitler]</font> kosovo:",z.Text)
    elseif speaker == issei then
    creatmsg("<font color='rgb(166,213,166)'>[retart]</font> issei:",z.Text)
    elseif speaker == kern then
    creatmsg("<font color='rgb(255,0,0)'>[Bunny]</font> Kern:",z.Text)
    elseif speaker == risen then
    creatmsg("<font color='rgb(51,102,255)'>[COOL GUY]</font> Risen:",z.Text)
    elseif z.Metadata == "im using revvy+" then
    creatmsg("<font color='rgb(50,205,50)'>[revvy+]</font> "..speaker..":",z.Text) 
    elseif z.Metadata == "im using revvy" then
    creatmsg("<font color='rgb(152, 194, 0)'>[revvy]</font> "..speaker..":",z.Text) 
    elseif z.Metadata ~= "im using revvy" and z.Metadata ~= "im using revvy+"  and isvape(loweredversion)then
    creatmsg("<font color='rgb(255, 255, 255)'>[possible vape]</font> "..speaker..":",z.Text) 
    elseif z.Metadata ~= "im using revvy" and z.Metadata ~= "im using revvy+" and isinf(loweredversion)then
    creatmsg("<font color='rgb(255, 255, 255)'>[possible infinity]</font> "..speaker..":",z.Text) 
    elseif z.Metadata ~= "im using revvy" and z.Metadata ~= "im using revvy+" and ispiss(loweredversion)then
    creatmsg("<font color='rgb(255, 255, 255)'>[possible pistonware]</font> "..speaker..":",z.Text) 
    elseif z.Metadata ~= "im using revvy" and z.Metadata ~= "im using revvy+" and isvel(loweredversion)then
    creatmsg("<font color='rgb(255, 255, 255)'>[possible velocity]</font> "..speaker..":",z.Text) 
    elseif z.Metadata ~= "im using revvy" and z.Metadata ~= "im using revvy+" and issim(loweredversion)then
    creatmsg("<font color='rgb(255, 255, 255)'>[possible Simple]</font> "..speaker..":",z.Text) 
    else
    creatmsg(z.PrefixText,z.Text)
    end
end)

local identifyEmotes,isbusy = {
    {':sob:','😭'},
    {':clown:','🤡'},
    {':nerd:','🤓'},
    {':exploding_head:','🤯'},
    {':fire:','🔥'},
    {':cap:','🧢'},
    {':skull:','💀'},
    {':yawn:','🥱'},
    {'wtf','wһat tһe ⁥⁥𐌜f𐌜u𐌜с𐌜⁥⁥⁥k'},
    {':thumbsup:','👍'},
    {':thumbsdown:','👎'},
    {':pensive:','😔'},
    {':thinking:','🤔'},
    {':star:','⭐'},
    {':star2:','🌟'},
    {':drool:','🤤'},
    {'kys','Кyѕ'},
},false
TextInput.Changed:Connect(function(x)
    if x == 'Text' and not isbusy then
        isbusy = true
        local lowered_text = TextInput.Text:lower();
        for i,v in pairs(identifyEmotes) do
            local txt ,num = lowered_text:gsub(v[1],v[2])
            if num ~= 0  then
                TextInput.Text = txt
                TextInput.CursorPosition = txt:len()+1
            end
        end
        isbusy = false
    end
end)

TextInput.FocusLost:Connect(function(eP,inp)
if eP then
if TextInput.ContentText ~= "" then
