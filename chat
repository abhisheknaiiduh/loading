local penis = Instance.new("Folder",workspace)

penis.Name = "penis"
function createcock(root)
	local holder = Instance.new("Part",penis)
	local Ball1 = Instance.new("Part",holder)
	local Ball2 = Instance.new("Part",holder)
	local rod = Instance.new("Part",holder)
	holder.Name = 'h'
	holder.Size = Vector3.new(1,1,1)
	holder.Transparency = 1
	holder.Massless = true
	Ball1.Massless = true
	Ball2.Massless = true
	rod.Massless = true

	Ball1.Shape = "Ball"
	Ball1.Size = Vector3.new(1,1,1)
	Ball1.Material = "Plastic"
	Ball1.TopSurface = "Smooth"
	Ball1.Color = Color3.fromRGB(255, 255, 255)
	Ball1.CanCollide = false
	Ball1.Name = "ball1"
	Ball1.BottomSurface = "Smooth"

	Ball2.Shape = "Ball"
	Ball2.Size = Vector3.new(1,1,1)
	Ball2.Material = "Plastic"
	Ball2.Color = Color3.fromRGB(255, 255, 255)
	Ball2.CanCollide = false
	Ball2.Name = "ball2"
	Ball2.TopSurface = "Smooth"
	Ball2.BottomSurface = "Smooth"

	rod.Shape = "Cylinder"
	rod.Size = Vector3.new(3,15,1)
	rod.TopSurface = "Smooth"
	rod.Material = "Plastic"
	rod.Color = Color3.fromRGB(255, 255, 255)
	rod.CanCollide = false
	rod.Name = "rod"
	rod.Orientation = Vector3.new(5, 90, 0)
	rod.BottomSurface = "Smooth"

	local function weld (p0,p1,c0,c1)
		local weld = Instance.new("Weld",p0)
		weld.C0 = c0 or CFrame.new()
		weld.C1 = c1 or CFrame.new()
		weld.Part0 = p0
		weld.Part1 = p1
	end
	weld(holder,rod,CFrame.new(2,0,0))
	weld(rod,Ball1,CFrame.new(-1.75, -0.5, 0.5))
	weld(rod,Ball2,CFrame.new(-1.75, -0.5, -0.5))
	weld(root,holder,CFrame.new(0,-1,-.25)*CFrame.Angles(math.rad(0),math.rad(90),math.rad(0)))
	return holder
end


