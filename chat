	elseif ChatGui.Position.X.Offset == -(xbase+1) then
		info = table.clone(info)
		info[5] = function()
			Close.Image = "http://www.roblox.com/asset/?id=6031094670"
		end
		ChatGui:TweenPosition(UDim2.new(0, 10,0.28, -10),unpack(info))
	end
end)

