local users = {}

local vapeusers = {}

local infusers = {}

local pissusers = {}

local velusers = {}

local simusers = {}

local fakeusers ={}

ts.MessageReceived:Connect(function(detect)
local speaker = tostring(detect.TextSource)
person = speaker:lower()
if detect.Metadata ~= "" then
if detect.Metadata == "im using revvy" then
table.insert(users, person)
end 
end
end)


ts.MessageReceived:Connect(function(detect)
local speaker = tostring(detect.TextSource)
person = speaker:lower()
if detect.Metadata ~= "" then
if detect.Metadata == "hide me" then
table.insert(fakeusers, person)
elseif detect.Metadata == "unhide me" then
local index = table.find(fakeusers, person)
table.remove(fakeusers, index)
end 
end
end)

local ts = game:GetService("TextChatService")
ts.MessageReceived:Connect(function(detect)
local speaker = tostring(detect.TextSource)
person = speaker:lower()
vxpe = detect.Text:lower()
if vxpe:match("vxpe") or vxpe:match("Vxpe") then
print(""..speaker.." might use vape!")
table.insert(vapeusers, person)
end 
end)


local ts = game:GetService("TextChatService")
ts.MessageReceived:Connect(function(detect)
local speaker = tostring(detect.TextSource)
person = speaker:lower()
pisston = detect.Text:lower()
if pisston:match("piston") or pisston:match("Piston") then
print(""..speaker.." might use Pistonware!")
table.insert(pissusers, person)
end 
end)


