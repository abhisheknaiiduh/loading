local function system(msg)
    local text = "<font color ='#da8541'>[SCRIPT]</font>: "..msg..""
    local TextMessage = Instance.new("TextLabel",DisplayMessages)
    local TextMessagePadding = Instance.new("UIPadding",TextMessage)

    TextMessage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextMessage.BackgroundTransparency = 1.000
    TextMessage.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextMessage.BorderSizePixel = 0
    TextMessage.Size = UDim2.new(1, 0, 0, 25)
    TextMessage.Font = Enum.Font.Gotham
    TextMessage.Text = text
    TextMessage.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextMessage.TextSize = 14.000
    TextMessage.TextWrapped = true
    TextMessage.RichText = true
    TextMessage.TextXAlignment = Enum.TextXAlignment.Left
    TextMessage.TextYAlignment = Enum.TextYAlignment.Top

    TextMessagePadding.PaddingLeft = UDim.new(0, 5)
    TextMessagePadding.PaddingTop = UDim.new(0, 5)
    DisplayMessages.CanvasPosition = Vector2.new(0,DisplayMessages.AbsoluteCanvasSize.Y)
end



local function invis()
    local text = ""
    local TextMessage = Instance.new("TextLabel",DisplayMessages)
    local TextMessagePadding = Instance.new("UIPadding",TextMessage)

    TextMessage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextMessage.BackgroundTransparency = 1.000
    TextMessage.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextMessage.BorderSizePixel = 0
    TextMessage.Size = UDim2.new(1, 0, 0, 25)
    TextMessage.Font = Enum.Font.Gotham
    TextMessage.Text = text
    TextMessage.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextMessage.TextSize = 14.000
    TextMessage.TextWrapped = true
    TextMessage.RichText = true
    TextMessage.TextXAlignment = Enum.TextXAlignment.Left
    TextMessage.TextYAlignment = Enum.TextYAlignment.Top

    TextMessagePadding.PaddingLeft = UDim.new(0, 5)
    TextMessagePadding.PaddingTop = UDim.new(0, 5)
    DisplayMessages.CanvasPosition = Vector2.new(0,DisplayMessages.AbsoluteCanvasSize.Y)
end

local function creatmsg(msg,md)
    local text = ""..msg.." "..md..""
    local TextMessage = Instance.new("TextLabel",DisplayMessages)
    local TextMessagePadding = Instance.new("UIPadding",TextMessage)

    TextMessage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextMessage.BackgroundTransparency = 1.000
    TextMessage.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextMessage.BorderSizePixel = 0
    TextMessage.Size = UDim2.new(1, 0, 0, 25)
    TextMessage.Font = Enum.Font.Gotham
    TextMessage.Text = text
    TextMessage.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextMessage.TextSize = 14.000
    TextMessage.TextWrapped = true
    TextMessage.RichText = true
    TextMessage.TextXAlignment = Enum.TextXAlignment.Left
    TextMessage.TextYAlignment = Enum.TextYAlignment.Top
    TextMessage.AutomaticSize = "Y"

    TextMessagePadding.PaddingLeft = UDim.new(0, 5)
    TextMessagePadding.PaddingTop = UDim.new(0, 5)
    DisplayMessages.CanvasPosition = Vector2.new(0,DisplayMessages.AbsoluteCanvasSize.Y)
end

system('GUI and GUI coding done by amnoob_ ,partial credits to me/rev for other coding')
invis()

ts.MessageReceived:Connect(function(z)
    local speaker = tostring(z.TextSource)
    if z.Metadata ~= "" then print(z.Metadata)end
    if speaker == owner then
    creatmsg("<font color='rgb(152, 194, 219)'>[SEXER]</font> <font color='rgb(0, 255, 0)'>Rev</font>:",z.Text)
    elseif speaker == xerv then
    creatmsg("<font color='rgb(152, 194, 219)'>[AC MOD]</font> xerv:",z.Text)
    elseif speaker == kos then
    creatmsg("<font color='rgb(152, 0, 0)'>[adolf hitler]</font> kosovo:",z.Text)
    elseif speaker == issei then
    creatmsg("<font color='rgb(166,213,166)'>[retart]</font> issei:",z.Text)
    elseif z.Metadata == "im using revvy" then
    creatmsg("<font color='rgb(152, 194, 0)'>[revvy+]</font> "..speaker..":",z.Text) 
    else
    creatmsg(z.PrefixText,z.Text)
    end
end)

local identifyEmotes,isbusy = {
    {':sob:','ðŸ˜­'},
    {':clown:','ðŸ¤¡'},
    {':nerd:','ðŸ¤“'},
    {':exploding_head:','ðŸ¤¯'},
    {':fire:','ðŸ”¥'},
    {':cap:','ðŸ§¢'},
    {':skull:','ðŸ’€'},
    {':yawn:','ðŸ¥±'},
    {'wtf','wÒ»at tÒ»e â¥â¥ðŒœfðŒœ.uðŒœ.ÑðŒœâ¥â¥â¥k'},
    {':thumbsup:','ðŸ‘'},
    {':thumbsdown:','ðŸ‘Ž'},
    {':pensive:','ðŸ˜”'},
    {':thinking:','ðŸ¤”'},
    {':star:','â­'},
    {':star2:','ðŸŒŸ'},
},false
TextInput.Changed:Connect(function(x)
    if x == 'Text' and not isbusy then
        isbusy = true
        local lowered_text = TextInput.Text:lower();
        for i,v in pairs(identifyEmotes) do
            local txt ,num = lowered_text:gsub(v[1],v[2])
            if num ~= 0  then
                TextInput.Text = txt
                TextInput.CursorPosition = txt:len()+1
            end
        end
        isbusy = false
    end
end)

TextInput.FocusLost:Connect(function(eP,inp)
    if eP then
        if TextInput.ContentText ~= "" then
            ts.ChatInputBarConfiguration.TargetTextChannel:SendAsync(TextInput.ContentText,"im using revvy")
            TextInput.Text = ""
        end
    end
end)

Inputbar.Changed:Connect(function(i)
	if i == "AbsoluteSize" and Inputbar.AbsoluteSize.Y >= 25 then
		local add = Inputbar.AbsoluteSize.Y - 25
		ChatGui.Size = UDim2.new(0, xbase, 0, add+250)
	end
end)

local UIS = game:GetService("UserInputService")
UIS.InputBegan:Connect(function(input)
if input.KeyCode == Enum.KeyCode.Slash then
wait()
TextInput:CaptureFocus()
end
end)
